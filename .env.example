# ----------------------------
# OpenEduCat - example .env
# ----------------------------
# Domain
DOMAIN=openeducat.ceredis.net
ADMIN_EMAIL=admin@ceredis.net

# PostgreSQL
POSTGRES_DB=odoo
POSTGRES_USER=odoo
POSTGRES_PASSWORD=SuperPostgresPass!

# Odoo admin password (config manager)
ODOO_ADMIN_PASSWORD=SuperOdooAdminPass!

# Dropbox / rclone remote token (use rclone config to create)
RCLONE_REMOTE_NAME=remote
RCLONE_DROPBOX_TOKEN={"access_token":"XXXX","token_type":"bearer","expiry":"0001-01-01T00:00:00Z"}

# Traefik / LetsEncrypt email
TRAEFIK_EMAIL=admin@ceredis.net
